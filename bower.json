{
  "name": "alche-tabcontainer",
  "version": "0.1.0",
  "homepage": "https://github.com/zeikman/alche-tabcontainer",
  "authors": [
    "Zeik Tan Yong Xiang <zeikxiang@gmail.com>"
  ],
  "description": "An element combine of polymer 'paper-tabs' and 'iron-pages' to implement 'dijit/layout/TabContainer' behavior like from dojotollkit",
  "main": "alche-tabcontainer.html",
  "keywords": [
    "polymer",
    "web-component",
    "web-components",
    "tabcontainer"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.0.0",
    "paper-tabs": "polymerelements/paper-tabs#^1.0.0",
    "iron-pages": "polymerelements/iron-pages#^1.0.0",
    "iron-a11y-keys": "polymerelements/iron-a11y-keys#^1.0.0"
  },
  "devDependencies": {
    "web-component-tester": "*"
  }
